{
  "name": "database",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "postinstall": "pnpm db:generate",
    "build": "tsup",
    "dev": "pnpm db:up && tsup --watch",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate:pg",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:migrate": "node dist/migrate.js"
  },
  "devDependencies": {
    "drizzle-kit": "^0.20.6",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "drizzle-orm": "^0.29.1",
    "postgres": "^3.4.3"
  }
}
